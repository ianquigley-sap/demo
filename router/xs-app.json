{
    "welcomeFile": "/cp.portal",
    "authenticationMethod": "route",
    "routes": [
        {
            "source": "^/app/(.*)$",
            "target": "/$1",
            "service": "html5-apps-repo-rt",
            "authenticationType": "xsuaa"
        },
        {
            "source": "^/sap/(.*)$",
            "target": "/sap/$1",
            "destination": "X08_0036",
            "authenticationType": "xsuaa"
        }
    ]
}